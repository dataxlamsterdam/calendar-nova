{
    "name": "dataxlamsterdan/calendar-nova",
    "description": "Calendar tool for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "calendar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tim de Kok",
            "email": "tim@dataxl.nl",
            "homepage": "https://paweldymek.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-google-calendar": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dataxl\\NovaCalendarTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dataxl\\NovaCalendarTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
